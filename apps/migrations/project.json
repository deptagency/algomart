{
  "root": "apps/migrations",
  "sourceRoot": "apps/migrations/src",
  "projectType": "application",
  "targets": {
    "up": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P tsconfig.app.json src/main.ts up",
        "cwd": "apps/migrations"
      }
    },
    "down": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P tsconfig.app.json src/main.ts down",
        "cwd": "apps/migrations"
      }
    },
    "latest": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P tsconfig.app.json src/main.ts latest",
        "cwd": "apps/migrations"
      }
    },
    "rollback": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P tsconfig.app.json src/main.ts rollback",
        "cwd": "apps/migrations"
      }
    },
    "list": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P tsconfig.app.json src/main.ts list",
        "cwd": "apps/migrations"
      }
    },
    "make": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P tsconfig.app.json src/main.ts make {args.name}",
        "cwd": "apps/migrations"
      }
    },
    "status": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P tsconfig.app.json src/main.ts status",
        "cwd": "apps/migrations"
      }
    },
    "currentVersion": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P tsconfig.app.json src/main.ts currentVersion",
        "cwd": "apps/migrations"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/migrations/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/migrations"],
      "options": {
        "jestConfig": "apps/migrations/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
