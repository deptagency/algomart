{
  "name": "@algomart/api",
  "version": "1.0.0-alpha.0",
  "private": true,
  "main": "dist/algomart-api.cjs.js",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "db:generate": "npm run knex -- migrate:make",
    "db:latest": "npm run knex -- migrate:latest",
    "db:rollback": "npm run knex -- migrate:rollback",
    "db:down": "npm run knex -- migrate:down",
    "db:up": "npm run knex -- migrate:up",
    "build": "rollup --config",
    "dev": "tsnd -r tsconfig-paths/register -r dotenv/config --quiet src/index.ts",
    "start": "node -r dotenv/config dist/index.js",
    "test": "jest",
    "knex": "node --require dotenv/config --require tsconfig-paths/register ../../node_modules/.bin/knex --knexfile ./src/configuration/knex-config.ts",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.1",
    "@types/crypto-js": "4.0.2",
    "@types/i18next-fs-backend": "1.1.2",
    "@types/node": "16.11.6",
    "@types/rosie": "0.0.40",
    "@types/uuid": "8.3.1",
    "nyc": "15.1.0",
    "rosie": "2.1.0",
    "sqlite3": "5.0.2",
    "testdouble": "3.16.3",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@algomart/schemas": "1.0.0-alpha.0",
    "@babel/runtime": "7.16.0",
    "@dinero.js/currencies": "2.0.0-alpha.8",
    "@fastify/ajv-compiler": "2.1.0",
    "@sendgrid/mail": "7.5.0",
    "@sinclair/typebox": "0.20.5",
    "ajv": "8.6.3",
    "ajv-formats": "2.1.1",
    "algosdk": "1.12.0",
    "crypto-js": "4.1.1",
    "dinero.js": "2.0.0-alpha.8",
    "dotenv": "10.0.0",
    "env-var": "7.1.1",
    "fastify": "3.22.1",
    "fastify-bearer-auth": "6.0.0",
    "fastify-error": "0.3.1",
    "fastify-plugin": "3.0.0",
    "fastify-schedule": "1.0.0",
    "fastify-sensible": "3.1.2",
    "fastify-swagger": "4.12.6",
    "got": "11.8.2",
    "http-errors": "1.8.0",
    "i18next": "21.4.0",
    "i18next-fs-backend": "1.1.1",
    "knex": "0.95.13",
    "objection": "3.0.0-alpha.5",
    "pg": "8.7.1",
    "pino": "7.1.0",
    "pino-http": "6.0.0",
    "pino-pretty": "7.1.0",
    "toad-scheduler": "1.6.0",
    "uuid": "8.3.2"
  }
}
